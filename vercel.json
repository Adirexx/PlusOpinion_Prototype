{
    "headers": [
        {
            "source": "/service-worker.js",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=0, must-revalidate"
                }
            ]
        },
        {
            "source": "/version.json",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=0, must-revalidate"
                }
            ]
        },
        {
            "source": "/(.*).html",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=0, must-revalidate"
                }
            ]
        },
        {
            "source": "/(.*).HTML",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=0, must-revalidate"
                }
            ]
        },
        {
            "source": "/(.*).js",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        },
        {
            "source": "/(.*).css",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        },
        {
            "source": "/(.*).png",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ],
    "rewrites": [
        {
            "source": "/feed",
            "destination": "/feed.html"
        },
        {
            "source": "/bookmarks",
            "destination": "/bookmarks.html"
        },
        {
            "source": "/categories",
            "destination": "/categories.html"
        },
        {
            "source": "/myspace",
            "destination": "/myspace.html"
        },
        {
            "source": "/workspace",
            "destination": "/workspace.html"
        },
        {
            "source": "/notifications",
            "destination": "/notifications.html"
        },
        {
            "source": "/myprofile",
            "destination": "/myprofile.html"
        },
        {
            "source": "/profile",
            "destination": "/profile.html"
        },
        {
            "source": "/onboarding",
            "destination": "/onboarding.html"
        },
        {
            "source": "/reset-password",
            "destination": "/reset-password.html"
        },
        {
            "source": "/change-password",
            "destination": "/change-password.html"
        },
        {
            "source": "/about",
            "destination": "/about.html"
        },
        {
            "source": "/support",
            "destination": "/support.html"
        },
        {
            "source": "/privacy-policy",
            "destination": "/privacy-policy.html"
        },
        {
            "source": "/terms-and-conditions",
            "destination": "/terms-and-conditions.html"
        }
    ]
}
